<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Brush.Background" Color="{StaticResource DarkColor.Background}"/>
    <SolidColorBrush x:Key="Brush.Surface" Color="{StaticResource DarkColor.Surface}"/>
    <SolidColorBrush x:Key="Brush.SurfaceAlt" Color="{StaticResource DarkColor.SurfaceAlt}"/>
    <SolidColorBrush x:Key="Brush.Border" Color="{StaticResource DarkColor.Border}"/>
    <SolidColorBrush x:Key="Brush.BorderStrong" Color="{StaticResource DarkColor.BorderStrong}"/>
    <SolidColorBrush x:Key="Brush.TextPrimary" Color="{StaticResource DarkColor.TextPrimary}"/>
    <SolidColorBrush x:Key="Brush.TextSecondary" Color="{StaticResource DarkColor.TextSecondary}"/>
    <SolidColorBrush x:Key="Brush.Accent" Color="{StaticResource DarkColor.Accent}"/>
    <SolidColorBrush x:Key="Brush.AccentHover" Color="{StaticResource DarkColor.AccentHover}"/>
    <SolidColorBrush x:Key="Brush.AccentPressed" Color="{StaticResource DarkColor.AccentPressed}"/>
    <SolidColorBrush x:Key="Brush.Danger" Color="{StaticResource DarkColor.Danger}"/>
    <SolidColorBrush x:Key="Brush.Success" Color="{StaticResource DarkColor.Success}"/>
    <SolidColorBrush x:Key="Brush.RowHover" Color="{StaticResource DarkColor.RowHover}"/>
</ResourceDictionary>
